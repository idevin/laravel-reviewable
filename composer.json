{
    "name": "idevin/reviewable",
    "description": "Reviewable Polymorphic Eloquent Models for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-reviewable",
        "Laravel Reviewable"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "i.devin@creotech.org",
        "homepage": "https://github.com/idevin/laravel-reviewable"
    }],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Reviewable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Reviewable\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Reviewable\\ReviewableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
